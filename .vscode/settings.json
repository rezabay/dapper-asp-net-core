{
    "typescript.referencesCodeLens.enabled": true,
    "debug.inlineValues": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/obj": true,
        ".vs": true,
        "*.sln": true,
        "*.csproj.user": true,
        ".idea/": true,
        "**/bin": true,
    },
    "search.exclude": {
        "**/bower_components": true,
        "coverage": true,
        "compiled": false,
        "wwwroot": true,
        "bin": true
    },
